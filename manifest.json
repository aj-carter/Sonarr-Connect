{
   "background": {
      "page": "background.html"
   },
   "browser_action": {
      "default_icon": "images/icon19.png",
      "default_popup": "popup.html",
      "default_title": "Sonarr Connect"
   },
   "content_scripts": [ {
      "js": [ "scripts/SickBeardConnect.js" ],
      "matches": [ "http://trakt.tv/show/*", "http://*.trakt.tv/show/*", "http://thetvdb.com/*series*", "http://*.thetvdb.com/*series*" ]
   } ],
   "default_locale": "en",
   "description": "SickBeard extension for Google Chrome.",
   "icons": {
      "128": "images/icon128.png",
      "16": "images/icon16.png",
      "48": "images/icon48.png"
   },
   
   "manifest_version": 2,
   "name": "Sonarr Connect",
   "options_page": "options.html",
   "permissions": [ "http://*/*", "https://*/*", "tabs", "notifications" ],
   
   "version": "0.1.21",
   "web_accessible_resources": [ "images/icon48.png", "images/no16.png", "css/sbconnect_content.css", "images/bg.gif", "images/yes16.png", "images/throbber.svg" ]
}
